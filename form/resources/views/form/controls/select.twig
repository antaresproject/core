<select name="{{ control.getName() }}" {{ control.getAttributes()|stringify }}>
    {% if control.hasEmptyValue() %}
        <option>{{ control.getEmptyValue() }}</option>
    {% endif %}
    {% for option in control.getValueOptions() %}
        <option value="{{ option.value }}" {% if option.selected %}selected{% endif %} {{ option.getAttributes()|stringify }}>
            {{ option.name }}
        </option>
    {% endfor %}
</select>